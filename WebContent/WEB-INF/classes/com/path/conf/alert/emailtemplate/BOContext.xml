<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="emailTemplateBO" factory-bean="alertServiceBeanFactory"  factory-method="getInstance" parent="baseBO">					
		<constructor-arg name="implClassName" value="com.path.alert.bo.emailtemplate.impl.EmailTemplateBOImpl" />
		<constructor-arg name="interfaceName" value="com.path.alert.bo.emailtemplate.EmailTemplateBO" />
		<constructor-arg name="remoteServiceName" value="emailTemplateBOService" />
		<property name="emailTemplateDAO" ref="emailTemplateDAO"></property>
	</bean>	


</beans>